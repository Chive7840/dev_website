{
  "name": "dev_website",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=20.0.0",
    "yarn": ">=1.0.0"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build --turbopack",
    "start": "next start",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint-staged": "lint-staged",
    "precommit": "yarn run lint-staged && yarn run typecheck",
    "cms:dev": "yarn run dev --prefix cms/sanity/developer-portfolio",
    "cms:deploy": "yarn run deploy --prefix cms/sanity/developer-portfolio",
    "cms:typecheck": "yarn run typecheck --prefix cms/sanity/developer-portfolio",
    "cms:check": "yarn run typecheck --prefix cms/sanity/developer-portfolio",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "generate:themes": "tsx scripts/generate-themes.ts",
    "prepare": "simple-git-hooks"
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@react-three/drei": "^10.7.6",
    "@react-three/fiber": "^9.4.0",
    "@rushstack/eslint-patch": "^1.14.0",
    "@sanity/client": "^7.12.0",
    "@sanity/image-url": "^1.2.0",
    "@storybook/nextjs": "^9.1.13",
    "@types/three": "^0.180.0",
    "ajv": "^8.17.1",
    "ajv-dist": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "clsx": "^2.1.1",
    "eslint-config-next": "^15.5.6",
    "groq": "^4.10.3",
    "next": "^16.1.1",
    "picomatch": "^4.0.3",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "three": "^0.180.0"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4.1.1",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.38.0",
    "@next/eslint-plugin-next": "^15.5.6",
    "@storybook/addon-a11y": "^9.1.13",
    "@storybook/addon-docs": "^9.1.13",
    "@storybook/addon-onboarding": "^9.1.13",
    "@storybook/nextjs": "^9.1.13",
    "@tailwindcss/postcss": "^4.1.14",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^16.0.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^24.8.1",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@typescript-eslint/eslint-plugin": "^8.46.1",
    "@typescript-eslint/parser": "^8.46.1",
    "autoprefixer": "^10.4.21",
    "culori": "^4.0.2",
    "eslint": "^9.38.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-storybook": "^9.1.13",
    "globals": "^16.4.0",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^29.7.0",
    "jsdom": "^27.1.0",
    "lint-staged": "^15.2.10",
    "playwright": "^1.56.1",
    "postcss": "^8.5.6",
    "prettier": "^3.4.2",
    "simple-git-hooks": "^2.11.1",
    "storybook": "^9.1.13",
    "tailwindcss": "^4.1.14",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --cache --fix",
      "prettier --check"
    ],
    "*.{mjs,cjs,css,md,json}": "prettier --check"
  },
  "simple-git-hooks": {
    "pre-commit": "yarn run precommit"
  }
}
