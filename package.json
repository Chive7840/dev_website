{
  "name": "dev_website",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=20.0.0",
    "yarn": ">=1.0.0"
  },
  "main": "src/root.jsx",
  "scripts": {
    "dev": "vite",
    "build": "react-router build",
    "preview": "vite preview",
    "start": "react-router-serve dist/server/index.js",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint-staged": "lint-staged",
    "precommit": "yarn run lint-staged && yarn run typecheck",
    "prepare": "simple-git-hooks"
  },
  "dependencies": {
    "@react-router/node": "^7.11.0",
    "@react-router/serve": "^7.11.0",
    "@react-three/drei": "^10.7.6",
    "@react-three/fiber": "^9.4.0",
    "@rushstack/eslint-patch": "^1.14.0",
    "@sanity/client": "^7.12.0",
    "@sanity/image-url": "^1.2.0",
    "@types/three": "^0.180.0",
    "@vitejs/plugin-react": "^5.1.2",
    "ajv": "^8.17.1",
    "ajv-dist": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "axios": "^1.13.2",
    "clsx": "^2.1.1",
    "eslint-plugin-react-refresh": "^0.4.26",
    "isbot": "^5.1.32",
    "picomatch": "^4.0.3",
    "pkg.json": "^2.0.9",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-router": "^7.11.0",
    "tailwind": "^4.0.0",
    "three": "^0.180.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.38.0",
    "@react-router/dev": "^7.11.0",
    "@tailwindcss/postcss": "^4.1.14",
    "@testing-library/dom": "^10.0.0",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^16.0.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^24.8.1",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@typescript-eslint/eslint-plugin": "^8.46.1",
    "@typescript-eslint/parser": "^8.46.1",
    "@vitest/browser-playwright": "^4.0.16",
    "autoprefixer": "^10.4.21",
    "babel-plugin-react-compiler": "^1.0.0",
    "eslint": "^9.38.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "globals": "^16.4.0",
    "jsdom": "^27.1.0",
    "lint-staged": "^15.2.10",
    "playwright": "^1.56.1",
    "postcss": "^8.5.6",
    "prettier": "^3.4.2",
    "sass": "^1.97.1",
    "simple-git-hooks": "^2.11.1",
    "tailwindcss": "^4.1.14",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "vite": "^7.3.0",
    "vitest": "^4.0.16",
    "zustand": "^5.0.9"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --cache --fix --no-error-on-unmatched-pattern",
      "prettier --write"
    ],
    "*.{mjs,cjs,css,md,json}": "prettier --write"
  },
  "simple-git-hooks": {
    "pre-commit": "yarn run precommit"
  }
}
